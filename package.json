{
  "name": "promdress",
  "version": "0.2.1",
  "description": "promise dresser for parse js sdk",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "lint": "eslint src/parse",
    "test": "nyc ava",
    "test-w": "nodemon -q -x 'npm test'",
    "build": "babel -d ./lib --presets babel-preset-es2015 ./src",
    "build-w": "babel -w -d ./lib --presets babel-preset-es2015 ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spacious/prom-dress.git"
  },
  "author": "spacious",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/spacious/prom-dress/issues"
  },
  "homepage": "https://github.com/spacious/prom-dress#readme",
  "dependencies": {
    "parse": "^1.6.12"
  },
  "devDependencies": {
    "ava": "^0.6.1",
    "babel-cli": "^6.2.0",
    "babel-eslint": "^4.1.6",
    "babel-preset-es2015": "^6.1.18",
    "eslint": "^1.10.3",
    "nodemon": "^1.8.1",
    "nyc": "^3.2.2"
  },
  "babel": {
    "ignore": [
      "node_modules/*"
    ]
  }
}
